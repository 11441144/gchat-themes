{
    "compilerOptions": {
        "jsx": "react",
        "module": "es6",
        "target": "es6",
        "rootDir": ".",
        "esModuleInterop": true
    },
    "types": [
        "chrome"
    ],
    "allowSyntheticDefaultImports": true,
    "typeRoots": [
        "node_modules/@types",
        "manual_typings"
    ],
    "paths": {
        "lodash/*": [
            "node_modules/@types/lodash-es/*"
        ],
    },
}