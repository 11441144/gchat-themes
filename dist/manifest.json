{
"manifest_version": 2,
    "name": "Google Chat Dark Mode",
    "description": "Custom google chat theme",
    "version": "0.0.1",
    "content_scripts": [
        {
            "matches": [
                "https://chat.google.com/*"
            ],
            "js": [
                "./content.js"
            ],
            "run_at": "document_end"
        }
    ]
}